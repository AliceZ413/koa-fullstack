{
  "name": "koa-fullstack",
  "version": "0.0.1",
  "description": "基于Koa和Vite的全栈框架",
  "main": "index.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development run-p dev:*",
    "dev:vite": "vite",
    "dev:server": "nodemon",
    "build": "npm run build:vite && npm run build:server",
    "build:vite": "vite build",
    "build:server": "tsc --project tsconfig.build.json",
    "start": "cross-env NODE_ENV=production node ./dist/server/src/app.js",
    "check": "biome check --apply ./src ./ui"
  },
  "keywords": [
    "koa",
    "koa.js",
    "vite",
    "csr",
    "fullstack",
    "web"
  ],
  "author": "Stary",
  "license": "ISC",
  "devDependencies": {
    "@biomejs/biome": "1.4.1",
    "@swc/core": "^1.3.100",
    "@types/koa": "^2.13.12",
    "@types/koa-compress": "^4.0.6",
    "@types/koa-mount": "^4.0.5",
    "@types/koa-session": "^6.4.5",
    "@types/koa-static": "^4.0.4",
    "@types/node": "^20.10.4",
    "@vitejs/plugin-vue": "^4.5.2",
    "@vitejs/plugin-vue-jsx": "^3.1.0",
    "cross-env": "^7.0.3",
    "naive-ui": "^2.36.0",
    "nodemon": "^3.0.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.1.1",
    "prisma": "^5.7.0",
    "sass": "^1.69.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "vite": "^5.0.8"
  },
  "dependencies": {
    "@prisma/client": "^5.7.0",
    "destr": "^2.0.2",
    "koa": "^2.14.2",
    "koa-body": "^6.0.1",
    "koa-compress": "^5.1.1",
    "koa-mount": "^4.0.0",
    "koa-session": "^6.4.0",
    "koa-static": "^5.0.0",
    "koa-tree-router": "^0.12.1",
    "ky": "^1.1.3",
    "normalize.css": "^8.0.1",
    "vue": "^3.3.12",
    "vue-router": "^4.2.5",
    "zod": "^3.22.4"
  }
}