{
  "name": "koa-fullstack",
  "version": "0.1.0",
  "license": "ISC",
  "author": "Stary",
  "keywords": [
    "koa",
    "koa.js",
    "vite",
    "next.js",
    "fullstack",
    "web"
  ],
  "scripts": {
    "clean": "rimraf dist build",
    "postinstall": "prisma generate",
    "db:push": "prisma db push",
    "dev": "run-p dev:remix dev:server",
    "dev:remix": "cross-env NODE_ENV=development remix watch",
    "dev:server": "cross-env NODE_ENV=development ts-node ./src/app.ts",
    "build": "npm run build:remix && npm run build:server",
    "build:remix": "cross-env NODE_ENV=production remix build",
    "build:server": "cross-env NODE_ENV=production tsc -p ./tsconfig.build.json",
    "preview:prod": "cross-env NODE_ENV=production node dist/app.js"
  },
  "dependencies": {
    "@koa/router": "^12.0.1",
    "@prisma/client": "^5.8.0",
    "@remix-run/css-bundle": "^2.5.1",
    "@remix-run/node": "^2.5.1",
    "@remix-run/react": "^2.5.1",
    "antd": "^5.13.1",
    "chalk": "^5.3.0",
    "crypto-js": "^4.2.0",
    "isbot": "^4",
    "koa": "^2.15.0",
    "koa-static": "^5.0.0",
    "ky": "^1.2.0",
    "loglevel": "^1.8.1",
    "loglevel-plugin-prefix": "^0.8.4",
    "normalize.css": "^8.0.1",
    "path-to-regexp": "^6.2.1",
    "react": "^18",
    "react-dom": "^18",
    "remix-koa-adapter": "^1.1.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@remix-run/dev": "^2.5.1",
    "@swc/cli": "^0.1.63",
    "@swc/core": "^1.3.104",
    "@types/crypto-js": "^4.2.1",
    "@types/koa": "^2.14.0",
    "@types/koa__router": "^12.0.4",
    "@types/koa-static": "^4.0.4",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "cross-env": "^7.0.3",
    "eslint": "^8",
    "npm-run-all": "^4.1.5",
    "prisma": "^5.8.0",
    "rimraf": "^5.0.5",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  }
}
