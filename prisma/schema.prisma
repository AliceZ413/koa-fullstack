generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model DashboardUser {
  id        String   @id @default(uuid())
  username  String
  password  String
  name      String
  email     String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt

  @@unique([username])
  @@index([username])
  @@map("dashboard_users")
}
